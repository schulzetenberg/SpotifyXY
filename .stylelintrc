{
    "extends": "stylelint-config-standard",
    "ignoreFiles": [
    ],
    "rules": {
        # Override defaults
        "indentation": 2,
        "no-descending-specificity": null,
        "no-empty-source": null,
        "selector-type-no-unknown": [true, { ignoreTypes: [
        "router-outlet",
        "mat-toolbar"
        ]}],
        "at-rule-no-unknown": [true, {
            "ignoreAtRules": [
                "last",
                "extend",
                "at-root",
                "debug",
                "warn",
                "error",
                "if",
                "else",
                "for",
                "each",
                "while",
                "mixin",
                "include",
                "content",
                "return",
                "function"
            ]
        }]
    }
}
